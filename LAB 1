import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum = 0;
		int[] a = new int[n];
        System.out.println("Input " + n + " values:");
        for (int i = 0; i < n; i++) {
            a[i] = sc.nextInt();
            sum = sum +a[i];
        }
        System.out.println("The sum of the input values is: " + sum); 
        
}
}
